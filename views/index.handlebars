            <div class='btn_out'>
                <button id="confirm" class="btn btn-outline-primary my-2 my-sm-0" type="submit">Confirm</button>
            </div>

        </div>


        <div id='attributes'>
            <div id='headline'><a id='headline_b'>Attributions Selected</a></div>

            <div class="custom-control custom-checkbox" style='margin:3% 5% 0 5%;'>
                <input type="checkbox" class="custom-control-input" id="1" checked='true'>
                <label class="custom-control-label" for="1" id="citedLabel" style='color:gray'>Conference</label>
                <div class='tuli' style='background-color:#A597FF'></div>
            </div>

            <div class="custom-control custom-checkbox" style='margin:3% 5% 0 5%;'>
                <input type="checkbox" class="custom-control-input" id="2">
                <label class="custom-control-label" for="2" id="abtLabel" style='color:gray'>Year</label>
                <div class='tuli' style='background-color:#A597FF'></div>
            </div>

            <div class="custom-control custom-checkbox" style='margin:3% 5% 0 5%;'>
                <input type="checkbox" class="custom-control-input" id="3">
                <label class="custom-control-label" for="3" id="confLabel" style='color:gray'>Paper Type</label>
                <div class='tuli' style='background-color:#FFAB7C'></div>

            </div>

            {{!-- <div class="custom-control custom-checkbox" style='margin:3% 5% 0 5%;'>
                <input type="checkbox" class="custom-control-input" id="3">
                <label class="custom-control-label" for="3" id="yearLabel" style='color:gray'>18:00 ~ 21:00</label>
                <div class='tuli' style='background-color:#EE89FF'></div>

            </div> --}}

            {{!-- <div class="custom-control custom-checkbox" style='margin:3% 5% 0 5%;'>
