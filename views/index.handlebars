<div id='left'>
    <div id='dataset'>
        <div id="headLine">
            <a id='headline_a'>Data Infomation</a>
        </div>
        <label style='color:rgb(124, 124, 124);margin:10px;display:inline-block;margin:2% 5% 1% 5%'>Dataset：
        </label>
        <div class="dropdown show" style="display:inline-block;">
            <a class="btn btn-sm dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="true" style='margin-top:-5px;'>
                Patent
            </a>
            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                <a class="dropdown-item" href="#">Chicago</a>
                <a class="dropdown-item" href="#">Simulation data</a>
            </div>
        </div>
        <div style='display:block'>
            <label style='color:rgb(124, 124, 124);display:inline-block;margin:0 0 1% 5%; '>Nodes
            </label>
            <label style='color:rgb(124, 124, 124);display:inline-block;margin:0 0 1% 20%;'>Edges
            </label>
            <label style='color:rgb(124, 124, 124);margin:0 0 1% 16%;'>Attributes
            </label>
        </div>

        <div style='disply:block;color:rgb(124, 124, 124)'>
            <a class='data_info' id='node_quantity'  style='margin-left:5%;width:50px;'>0</a>
            <a class='data_info' id='edge_quantity'  style='margin-left:15%;width:70px;'>0</a>
            <a class='data_info' id='attr_quantity'  style='margin-left:15%;width:50px;'>0</a>
        </div>

    </div>

    <div id='control_pannel'>
        <div id="headLine">
            <a id='headline_a'>Control Pannel</a>
        </div>
        <div id='controlInner'>
            <div id='headline'><a id='headline_b'>Model Parameter</a></div>

            <div class='slider_out'>
                <span class='slider_span'>WT:
                </span>
                <input type="text" id="times_text" class='slider_input' value=5>
                <div id='walk_times' class='slider'></div>
                <span class='rangeLeft'>35</span>
            </div>
            
            <div class='slider_out'>
                <span class='slider_span'>WL:
                </span>
                <input type="text" id="length_text" class='slider_input' value=5>
                <div id='walk_length' class='slider'></div>
                <span class='rangeLeft'>35</span>
            </div>
            {{!-- <div id='radio_out'>
                <span class='slider_span'>RT:</span>
                <div class="custom-control custom-radio custom-control-inline" style='margin:5% 1% 0 5%;color:gray'>
                    <input type="radio" id="ret_True" name="customRadioInline_return" class="custom-control-input">
                    <label class="custom-control-label" for="ret_True">True</label>
                </div>
                <div class="custom-control custom-radio custom-control-inline"
                    style='margin-left:0%;margin-top:3%;color:gray'>
